/* Dark mode search and input improvements */
.dark input::placeholder,
.dark textarea::placeholder {
  color: rgba(255, 255, 255, 0.7) !important;
  opacity: 1 !important;
}

/* Dark mode scroll button improvements */
.dark .scroll-button-icon {
