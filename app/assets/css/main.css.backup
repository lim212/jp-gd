.dark .bg-gray-900 .text-white,
.dark .bg-gray-800 .text-white,
.dark .bg-slate-900 .text-white,
.dark .bg-slate-800 .text-white {
  color: #ffffff !important;
  text-shadow: 0 2px 4px rgba(0, 0, 0, 0.9) !important;
  font-weight: 600 !important;
}

/* Dark mode gradient background text improvements */
.dark .bg-gradient-to-r .text-white,
.dark .bg-gradient-to-l .text-white,
.dark .bg-gradient-to-t .text-white,
.dark .bg-gradient-to-b .text-white {
  color: #ffffff !important;
  text-shadow: 0 1px 3px rgba(0, 0, 0, 0.8) !important;
  font-weight: 600 !important;
}

/* Dark mode overlay text improvements */
.dark .bg-black\/60 .text-white,
.dark .bg-black\/50 .text-white,
.dark .bg-black\/40 .text-white,
