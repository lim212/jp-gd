/* Flag icon alignment for language buttons */
.flag-icon {
  width: 24px;
  height: 24px;
  display: inline-flex;
  align-items: center;
  justify-content: center;
}


/* Mobile/small-screen optimization: hide decorative elements and heavy animations */
@media (max-width: 768px) {
  /* Explicitly mark decorative elements to hide on small screens */
  .decorative-mobile { display: none !important; }

  /* Disable heavy animated backgrounds and big decorative overlays */
  .animated-bg, .parallax-bg, .hero-bg { background-image: none !important; animation: none !important; }
  .trusted-card::before, .trusted-card::after { animation: none !important; background: none !important; content: none !important; }
}

/* Data Saver mode: keep UI lightweight */
html.data-saver, html[data-data-saver="1"] {
  /* Reduce/disable non-essential visuals globally when Data Saver is active */
  background-image: none !important;
}
html.data-saver .decorative-only,
html[data-data-saver="1"] .decorative-only,
html.data-saver .animated-bg,
html[data-data-saver="1"] .animated-bg,
html.data-saver .parallax-bg,
html[data-data-saver="1"] .parallax-bg,
html.data-saver .hero-bg,
html[data-data-saver="1"] .hero-bg {
  background-image: none !important;
  animation: none !important;
}
html.data-saver .trusted-card::before,
html.data-saver .trusted-card::after,
html[data-data-saver="1"] .trusted-card::before,
html[data-data-saver="1"] .trusted-card::after {
  animation: none !important;
  background: none !important;
  content: none !important;
}

/* Text-only mode: aggressively hide media to show only text */
html.data-saver-text img,
html[data-data-saver-level="text"] img,
html.data-saver-text picture,
html[data-data-saver-level="text"] picture,
html.data-saver-text video,
html[data-data-saver-level="text"] video,
html.data-saver-text canvas,
html[data-data-saver-level="text"] canvas,
html.data-saver-text svg.image,
html[data-data-saver-level="text"] svg.image {
  display: none !important;
}


/* Desktop visual improvements (Aug 2025)
   - TrustedPartners: larger logos and gaps on wide screens
   - Footer: more gap on very large screens
   - Blog: improved desktop typography scale & spacing */
@media (min-width: 1024px) {
  /* Trusted Partners: more generous logo sizing and padding on desktop */
  .trusted-partners-swiper .partner-card { padding: 1rem; }
  .trusted-partners-swiper .partner-logo { max-height: 64px; }
}
@media (min-width: 1280px) {
  .trusted-partners-swiper .partner-card { padding: 1.1rem; }
  .trusted-partners-swiper .partner-logo { max-height: 72px; }
  .trusted-partners-swiper .swiper-slide .grid { gap: 1.25rem; }

  /* Footer widgets: add breathing room on very large screens */
  .site-footer-widgets { gap: 1.75rem; }
}

/* Blog desktop readability enhancements */
@media (min-width: 1024px) {
  .post-content { font-size: 1.075rem !important; line-height: 1.9 !important; }
  .post-content p { margin-block-end: 1.1rem; }
}
@media (min-width: 1280px) {
  .post-content h1 { font-size: 32px !important; }
  .post-content h2 { font-size: 26px !important; }
  .post-content h3 { font-size: 22px !important; }
}
