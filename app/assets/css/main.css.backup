@import "tailwindcss";
@import "@nuxt/ui-pro";
@import "./section-spacing-fix.css";
@import "./scroll-buttons.css";
@import "./spacing-optimization.css";

/* Content source and theme configuration */
/* @source "../../../content/"; */

/* @theme {
  --font-sans: 'Poppins', 'Inter', sans-serif;
  --font-nunito: 'Poppins', 'Inter', sans-serif;
  --font-exo: 'Poppins', 'Inter', sans-serif;
  --font-plus-jakarta-sans: 'Poppins', 'Inter', sans-serif;
  --font-montserrat: 'Poppins', 'Inter', sans-serif;
} */

:root {
  /* Light mode refreshed palette — clean, lively, professional */
  /* Core backgrounds */
  --bg: #F8FAFC;           /* Light slate */
  --surface: #FFFFFF;      /* Pure white surfaces */
  --subtle-bg: #F1F5F9;    /* Subtle section background */

  /* Text colors */
  --text: #0F172A;         /* Slate-900 */
  --text-secondary: #475569; /* Slate-600 */
  --heading: #0B1220;      /* Deep heading */

  /* Links (Brand accent - Professional Blue) */
  --link: #3b82f6;         /* Blue 500 */
  --link-hover: #2563eb;   /* Blue 600 */

  /* Call-to-Action (Professional Blue) */
  --cta-bg: #3b82f6;       /* Blue 500 */
  --cta-bg-hover: #2563eb; /* Blue 600 on hover */
  --cta-text: #FFFFFF;     /* White text on Blue for perfect contrast */

  /* Borders and separators */
  --border: #E5E7EB;       /* Neutral light border */

  /* Icons */
  --icon: #0F172A;         /* Match text */
  --icon-hover: #000000;   /* Pure black on hover */

  /* Existing UI token fallback */
  --ui-bg: var(--bg);

  /* Page background gradient (subtle, airy) */
  --bg-gradient: radial-gradient(1100px 520px at 110% -10%, color-mix(in oklab, var(--cta-bg), #fff 96%), transparent 55%),
                  radial-gradient(900px 420px at -10% 0%, color-mix(in oklab, var(--link), #fff 97%), transparent 60%),
                  linear-gradient(180deg, color-mix(in oklab, #FFFFFF, var(--bg) 80%), var(--bg));
}

/* Base typography */
html, body {
  line-height: 1.6;
}

.dark {
  /* Dark mode overrides - refreshed cool palette */
  --bg: #0b0f14;
  --surface: #11161c;
  --subtle-bg: #0d1419;

  --text: #e6edf3;
  --text-secondary: #9fb1c1;
  --heading: #eaf2ff;

  --link: #58a6ff;
  --link-hover: #79c0ff;

  --cta-bg: #22c55e;
  --cta-bg-hover: #16a34a;
  --cta-text: #FFFFFF;

  --border: #26313e;

  --icon: #e6edf3;
  --icon-hover: #FFFFFF;

  --ui-bg: var(--bg);

  /* Page background gradient (dark) */
  --bg-gradient: radial-gradient(1100px 520px at 110% -10%, color-mix(in oklab, var(--link), transparent 82%), transparent 55%),
                  radial-gradient(900px 420px at -10% 0%, color-mix(in oklab, var(--cta-bg), transparent 84%), transparent 60%),
                  linear-gradient(180deg, color-mix(in oklab, #0b0f14, #11161c 50%), var(--bg));
}

/* Base application colors */
html, body {
  background-color: var(--bg);
  color: var(--text);
  background-image: var(--bg-gradient);
  background-repeat: no-repeat;
  transition: background-color .25s ease, color .2s ease, border-color .2s ease, background-image .3s ease;
}

/* Typography */
h1, h2, h3, h4, h5, h6 {
  color: var(--heading);
}

/* Secondary text helper */
.text-secondary, .muted, .subtitle {
  color: var(--text-secondary);
}

/* Links */
a[href]:not(.btn-cta):not(.no-link-style) {
  color: var(--link) !important;
}
a[href]:not(.btn-cta):not(.no-link-style):hover,
a[href]:not(.btn-cta):not(.no-link-style):focus {
  color: var(--link-hover) !important;
}

/*
  Unified Box Utilities
  - Use to give all cards/boxes a consistent, modern look across light/dark, mobile/desktop
  - Apply class="ui-box" to container elements or UPageCard via class prop
*/
.ui-box {
  /* Base surface */
  background: color-mix(in oklab, var(--surface), transparent 0%);
  border: 1px solid color-mix(in oklab, var(--border), transparent 0%);
  border-radius: 16px; /* rounded-2xl */
  box-shadow: 0 6px 24px -6px color-mix(in oklab, #000, transparent 85%);
  transition: box-shadow .25s ease, transform .2s ease, background-color .25s ease, border-color .25s ease;
  backdrop-filter: saturate(120%) blur(2px);
}
.ui-box:hover {
  box-shadow: 0 10px 32px -6px color-mix(in oklab, #000, transparent 80%);
  transform: translateY(-2px);
}
.ui-box:focus-within {
  outline: none;
  box-shadow: 0 0 0 3px color-mix(in oklab, var(--link), transparent 60%), 0 10px 32px -6px color-mix(in oklab, #000, transparent 80%);
}
@media (prefers-reduced-motion: reduce) {
  .ui-box { transition: background-color .1s ease, border-color .1s ease; }
  .ui-box:hover { transform: none; }
}

/* Softer variant for content heavy sections */
.ui-box-soft {
  background: linear-gradient(180deg,
    color-mix(in oklab, var(--surface), transparent 0%),
    color-mix(in oklab, var(--subtle-bg), transparent 0%) 100%);
  border: 1px solid color-mix(in oklab, var(--border), transparent 0%);
  border-radius: 20px; /* rounded-3xl */
  box-shadow: 0 4px 18px -6px color-mix(in oklab, #000, transparent 87%);
  transition: box-shadow .25s ease, transform .2s ease, background .3s ease;
}
.ui-box-soft:hover { transform: translateY(-2px); box-shadow: 0 10px 28px -6px color-mix(in oklab, #000, transparent 80%); }

/* Accent border variant */
.ui-box-accent {
  border: 1px solid color-mix(in oklab, var(--link), transparent 50%);
  box-shadow: 0 6px 22px -6px color-mix(in oklab, var(--link), transparent 70%);
}

/* Partner card tune-up (scoped component also applies Tailwind, this is a gentle enhancement) */
.trusted-partners-swiper .partner-card {
  padding: .75rem; /* better spacing for logos */
}
.trusted-partners-swiper .partner-logo {
  max-height: 44px;
}
@media (min-width: 768px) {
  .trusted-partners-swiper .partner-logo { max-height: 54px; }
}

/* ================
   Global Icon/Loader Utilities
   - Centering, sizing, and non-overlap rules
   - Usable across all pages and inside blog .post-content
   ================ */
.icon-center, .post-content .icon-center {
  display: inline-grid;
  place-items: center;
  text-align: center;
  width: 100%;
}

.icon-stack-safe, .post-content .icon-stack-safe {
  display: inline-grid;
  justify-items: center;
  align-items: center;
  gap: clamp(8px, 2vw, 16px);
  width: 100%;
}

/* Default loader variables */
:root {
  --icon-size: 64px;
  --icon-color: var(--cta-bg);
  --icon-bg: color-mix(in oklab, var(--icon-color), #fff 90%);
}

/* Default smaller size on very small screens (ignored when inline style overrides) */
@media (max-width: 480px) {
  .icon-center, .post-content .icon-center {
    --icon-size: 56px;
  }
}

/* Book Loading (CSS only) */
.jp-book-loader {
  position: relative;
  width: var(--icon-size);
  height: calc(var(--icon-size) * 0.72);
  perspective: 600px;
}
.jp-book-loader .book {
  position: relative;
  width: 100%;
  height: 100%;
  border-radius: 8px;
  background: linear-gradient(135deg, var(--icon-bg), #fff);
  box-shadow: 0 6px 18px rgba(0,0,0,.1), inset 0 0 0 2px color-mix(in oklab, var(--icon-color), #000 10%);
}
.jp-book-loader .spine {
  position: absolute;
  left: 0;
  top: 0;
  width: 10%;
  height: 100%;
  background: color-mix(in oklab, var(--icon-color), #000 20%);
  border-top-left-radius: 8px;
  border-bottom-left-radius: 8px;
}
.jp-book-loader .page {
  position: absolute;
  top: 6%;
  bottom: 6%;
  left: 12%;
  right: 4%;
  background: #fff;
  border-radius: 6px;
  box-shadow: 0 1px 0 rgba(0,0,0,.06);
  transform-origin: left center;
}
.jp-book-loader .page.animate {
  animation: jpPageFlip 1.2s ease-in-out infinite;
}
.jp-book-loader .page.p2 { animation-delay: .2s; }
.jp-book-loader .page.p3 { animation-delay: .4s; }

@keyframes jpPageFlip {
  0%   { transform: rotateY(0deg); }
  40%  { transform: rotateY(-160deg); }
  60%  { transform: rotateY(-160deg); }
  100% { transform: rotateY(0deg); }
}

/* Cool Loading (conic spinner) */
.jp-cool-loader {
  width: var(--icon-size);
  height: var(--icon-size);
  border-radius: 50%;
  background: conic-gradient(from 0deg, var(--icon-color) 0 60%, transparent 60% 100%);
  -webkit-mask-image: radial-gradient(closest-side, transparent 58%, #000 60%);
          mask-image: radial-gradient(closest-side, transparent 58%, #000 60%);
  animation: jpSpin 1s linear infinite;
  filter: drop-shadow(0 6px 14px rgba(0,0,0,.12));
}
@keyframes jpSpin { to { transform: rotate(360deg); } }

/* Progress ring base */
.jp-progress {
  --ring-size: var(--icon-size);
  --ring-color: var(--icon-color);
  width: var(--ring-size);
  height: var(--ring-size);
  display: grid;
  place-items: center;
  position: relative;
}
.jp-progress svg {
  width: 100%;
  height: 100%;
  transform: rotate(-90deg);
}
.jp-progress .bg {
  stroke: color-mix(in oklab, var(--ring-color), #000 88%);
  opacity: .15;
}
.jp-progress .bar {
  stroke: var(--ring-color);
  transition: stroke-dashoffset .3s ease;
}
.jp-progress .label {
  position: absolute;
  inset: 0;
  display: grid;
  place-items: center;
  text-align: center;
  font-weight: 600;
  color: var(--text);
}
.jp-progress .label small {
  display: block;
  font-weight: 500;
  font-size: 0.8rem;
  color: var(--text-secondary);
}

/* Respect reduced motion */
@media (prefers-reduced-motion: reduce) {
  .jp-cool-loader { animation-duration: 2.4s; }
  .jp-book-loader .page.animate { animation-duration: 2s; }
}

/* Divider */
hr {
  border-color: var(--border);
}

/* Header surface and dropdowns */
#stickyHeader {
  background-color: var(--surface);
  color: var(--text);
  border-bottom: 1px solid var(--border);
}
#stickyHeader .menu, #stickyHeader .dropdown, #stickyHeader .popover, #stickyHeader .group-hover\:block, #stickyHeader .bg-white {
  background-color: var(--surface) !important;
  color: var(--text);
  border-color: var(--border) !important;
}

/* Map common utility classes to theme tokens */
.bg-white {
  background-color: var(--surface) !important;
}
.text-gray-800, .text-gray-700, .text-black {
  color: var(--text) !important;
}
.border-gray-200, .border-gray-100 {
  border-color: var(--border) !important;
}

/* CTA button utility */
.btn-cta {
  background-color: var(--cta-bg);
  color: var(--cta-text);
  border: 1px solid color-mix(in oklab, var(--cta-bg), #000 15%);
}
.btn-cta:hover,
.btn-cta:focus {
  background-color: var(--cta-bg-hover);
}

/* Icons using currentColor (only when no explicit text-* utility provided) */
svg:not([class*="text-"]), [class^="i-"]:not([class*="text-"]), [class*=" i-"]:not([class*="text-"]) {
  color: var(--icon);
}
svg:not([class*="text-"]):hover, [class^="i-"]:not([class*="text-"]):hover, [class*=" i-"]:not([class*="text-"]):hover {
  color: var(--icon-hover);
}
/* Subtle icon interactions */
[class^="i-"], [class*=" i-"], svg { transition: color .2s ease, transform .2s ease, filter .2s ease; }
[class^="i-"]:hover, [class*=" i-"]:hover, svg:hover { transform: translateY(-1px); }
.dark [class^="i-"]:hover, .dark [class*=" i-"]:hover, .dark svg:hover { filter: drop-shadow(0 0 8px rgba(255,255,255,0.09)); }

/* Ensure icons look great on dark even if text-blue-* is used */
.dark svg[class*="text-blue-"],
.dark [class^="i-"][class*="text-blue-"],
.dark [class*=" i-"][class*="text-blue-"] {
  color: var(--icon) !important;
}
.dark svg[class*="text-blue-"]:hover,
.dark [class^="i-"][class*="text-blue-"]:hover,
.dark [class*=" i-"][class*="text-blue-"]:hover {
  color: var(--icon-hover) !important;
}

/* Subtle banners/sliders background helpers */
.banner-dark, .slider-dark {
  background-color: var(--surface);
}


/* Additional utility mappings for theme consistency */
.text-gray-600, .text-gray-500, .text-gray-400, .text-gray-300 { color: var(--text-secondary) !important; }
.bg-gray-100, .bg-gray-50 { background-color: var(--subtle-bg) !important; }

/* Extra helpers to align Nuxt UI Pro utilities with theme tokens */
.bg-elevated { background-color: var(--surface) !important; }
.bg-elevated\/50 { background-color: color-mix(in oklab, var(--surface), transparent 50%) !important; }
.ring-default { --tw-ring-color: var(--border) !important; }
.border-default { border-color: var(--border) !important; }
/* Normalize common dark utilities to theme tokens */
.dark .bg-gray-800 { background-color: var(--surface) !important; }
.dark .bg-gray-800\/90 { background-color: color-mix(in oklab, var(--surface), transparent 10%) !important; }
.dark .bg-gray-900 { background-color: var(--bg) !important; }
.dark .hover\:bg-gray-800:hover { background-color: var(--surface) !important; }
.dark .border-gray-700, .dark .border-gray-600 { border-color: var(--border) !important; }

/* Adjust blue-tint helpers in dark mode to subtle glaze */
.dark .hover\:bg-blue-50:hover { background-color: rgba(29, 161, 242, 0.08) !important; }
.dark .bg-blue-50 { background-color: rgba(29, 161, 242, 0.06) !important; }

/* Override gradient strips in header to dark surfaces */
.dark #stickyHeader .bg-gradient-to-r { background-image: linear-gradient(90deg, #0b0f14, #11161c) !important; }

/* Map semi-transparent white backgrounds to dark surfaces in dark mode */
.dark .bg-white\/90 { background-color: rgba(22, 27, 34, 0.9) !important; }
.dark .bg-white\/80 { background-color: rgba(22, 27, 34, 0.8) !important; }
.dark .bg-white\/70 { background-color: rgba(22, 27, 34, 0.7) !important; }

/* Fix white and amber backgrounds in dark mode */
.dark .bg-white { background-color: var(--surface) !important; }
.dark .bg-gradient-to-br.from-amber-50.to-white {
  background-image: linear-gradient(to bottom right, var(--surface), var(--bg)) !important;
}
.dark .bg-gradient-to-br.from-white.to-amber-50 {
  background-image: linear-gradient(to bottom right, var(--surface), var(--bg)) !important;
}
.dark .bg-amber-50 { background-color: var(--surface) !important; }
.dark .bg-amber-100 { background-color: color-mix(in oklab, var(--surface), #000 10%) !important; }
.dark .hover\:bg-amber-50:hover { background-color: color-mix(in oklab, var(--surface), #fff 5%) !important; }
.dark .hover\:bg-amber-100:hover { background-color: color-mix(in oklab, var(--surface), #fff 10%) !important; }

/* Fix amber borders in dark mode */
.dark .border-amber-200 { border-color: color-mix(in oklab, var(--border), #000 10%) !important; }
.dark .hover\:border-amber-400:hover { border-color: color-mix(in oklab, var(--border), #fff 10%) !important; }

/* Fix amber text in dark mode for better visibility */
.dark .text-amber-600, .dark .text-amber-700 { color: var(--link) !important; }
.dark .hover\:text-amber-700:hover { color: var(--link-hover) !important; }


/* THEME: Light/Dark tokens override and accessibility (appended) */
:root {
  /* Light mode refreshed palette — clean, lively, professional (synced) */
  /* Core backgrounds */
  --bg: #F8FAFC;
  --surface: #FFFFFF;
  --subtle-bg: #F1F5F9;

  /* Text colors */
  --text: #0F172A;
  --text-secondary: #475569;
  --heading: #0B1220;

  /* Links (Professional Blue accent) */
  --link: #3b82f6;
  --link-hover: #2563eb;

  /* Call-to-Action (Professional Blue) */
  --cta-bg: #3b82f6;
  --cta-bg-hover: #2563eb;
  --cta-text: #FFFFFF;

  /* Borders and separators */
  --border: #E5E7EB;

  /* Icons */
  --icon: #0F172A;
  --icon-hover: #000000;

  /* UI tokens */
  --ui-bg: var(--bg);
  --ui-primary: #3b82f6;
  --bg-gradient: radial-gradient(1100px 520px at 110% -10%, color-mix(in oklab, var(--cta-bg), #fff 96%), transparent 55%),
                  radial-gradient(900px 420px at -10% 0%, color-mix(in oklab, var(--link), #fff 97%), transparent 60%),
                  linear-gradient(180deg, color-mix(in oklab, #FFFFFF, var(--bg) 80%), var(--bg));
}

.dark {
  /* Dark mode overrides - refreshed cool palette */
  --bg: #0b0f14;
  --surface: #11161c;
  --subtle-bg: #0d1419;

  --text: #e6edf3;
  --text-secondary: #9fb1c1;
  --heading: #eaf2ff;

  --link: #58a6ff;
  --link-hover: #79c0ff;

  --cta-bg: #22c55e;
  --cta-bg-hover: #16a34a;
  --cta-text: #FFFFFF;

  --border: #26313e;

  --icon: #e6edf3;
  --icon-hover: #ffffff;

  /* UI tokens */
  --ui-bg: var(--bg);
  --ui-primary: #58a6ff;
}

/* Focus visible for accessibility */
:focus-visible {
  outline: 2px solid currentColor;
  outline-offset: 3px;
}


/* System color scheme and accent for better native UI integration */
:root { color-scheme: light dark; accent-color: var(--link); }
.dark { color-scheme: dark; }
.light { color-scheme: light; }

/* Text selection colors for better UX in both modes */
::selection { background-color: color-mix(in oklab, var(--link), #000 20%); color: #ffffff; }


/* Light mode remap: Tailwind blue utilities -> brand gold palette */
html.light .text-blue-600, html:not(.dark) .text-blue-600 { color: var(--link) !important; }
html.light .hover\:text-blue-600:hover, html:not(.dark) .hover\:text-blue-600:hover { color: var(--link-hover) !important; }
html.light .text-blue-500, html:not(.dark) .text-blue-500 { color: var(--link) !important; }
html.light .text-blue-400, html:not(.dark) .text-blue-400 { color: color-mix(in oklab, var(--link), #000 15%) !important; }
html.light .text-blue-800, html:not(.dark) .text-blue-800 { color: color-mix(in oklab, var(--link), #000 30%) !important; }

/* Dark mode text-white fixes - ensure white text is visible on dark backgrounds */
.dark .text-white {
  color: #ffffff !important;
  text-shadow: 0 1px 2px rgba(0, 0, 0, 0.8) !important;
}

/* Dark mode text visibility improvements for cards and containers */
.dark .bg-black\/60 .text-white,
.dark .bg-black\/50 .text-white,
.dark .bg-black\/40 .text-white {
  color: #ffffff !important;
  text-shadow: 0 2px 4px rgba(0, 0, 0, 0.9) !important;
  font-weight: 600 !important;
}

/* Dark mode button text improvements */
.dark .bg-gradient-to-r .text-white,
.dark .bg-blue-500 .text-white,
.dark .bg-green-500 .text-white,
.dark .bg-blue-500 .text-white {
  color: #ffffff !important;
  text-shadow: 0 1px 2px rgba(0, 0, 0, 0.7) !important;
  font-weight: 600 !important;
}

/* Dark mode icon visibility improvements */
.dark .text-white svg,
